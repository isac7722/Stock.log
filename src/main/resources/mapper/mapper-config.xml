<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="callSettersOnNulls" value="true" />
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>


    <mappers>
        <mapper resource="mapper/notes/notes.xml" />
        <mapper resource="mapper/strategies/strategies.xml"/>
        <mapper resource="mapper/journals/journals.xml"/>
        <mapper resource="mapper/journals/trades.xml"/>
    </mappers>

</configuration>